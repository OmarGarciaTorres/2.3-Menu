import machine
import utime
from machine import Pin, I2C
import ssd1306
import framebuf

# Configuraci√≥n de los pines de los botones
PIN_ARRIBA = 1
PIN_ABAJO = 15
PIN_SELECCIONAR = 0

# Configurar el I2C para la SSD1306 (pines SDA y SCL)
i2c = I2C(0, scl=Pin(17), sda=Pin(16))  # Configura el bus I2C
oled = ssd1306.SSD1306_I2C(128, 64, i2c)  # Configura la pantalla OLED SSD1306

# Configurar los pines de los botones como entrada con pull-down
pin_arriba = machine.Pin(PIN_ARRIBA, machine.Pin.IN, machine.Pin.PULL_DOWN)
pin_abajo = machine.Pin(PIN_ABAJO, machine.Pin.IN, machine.Pin.PULL_DOWN)
pin_seleccionar = machine.Pin(PIN_SELECCIONAR, machine.Pin.IN, machine.Pin.PULL_DOWN)

