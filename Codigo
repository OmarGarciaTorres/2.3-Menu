plain_bytes = [M1, M2, M3, S1, S2, S3]
current_state = 0  # Estado inicial: M1

def display_state(state_index):
    oled.fill(0)
    buf = bytearray(plain_bytes[state_index])
    fb = framebuf.FrameBuffer(buf, 128, 64, framebuf.MONO_HLSB)
    oled.blit(fb, 0, 0)
    oled.show()

while True:
    # Verificar el bot√≥n de arriba
    if pin_arriba.value() == 1:
        if current_state == 0:  # Est√°s en M1, puedes bajar a M2
            current_state = 1
            display_state(current_state)
        elif current_state == 1:  # Est√°s en M2, puedes bajar a M3
            current_state = 2
            display_state(current_state)
        elif current_state == 3:  # Est√°s en S1, seleccionar para volver a M1
            current_state = 0
            display_state(current_state)
        elif current_state == 4:  # Est√°s en S2, seleccionar para volver a M2
            current_state = 1
            display_state(current_state)
        elif current_state == 5:  # Est√°s en S3, seleccionar para volver a M3
            current_state = 2
            display_state(current_state)

        while pin_arriba.value() == 1:
            utime.sleep_ms(10)

    # Verificar el bot√≥n de abajo
    elif pin_abajo.value() == 1:
        if current_state == 2:  # Est√°s en M3, puedes subir a M2
            current_state = 1
            display_state(current_state)
        elif current_state == 1:  # Est√°s en M2, puedes subir a M1
            current_state = 0
            display_state(current_state)
        elif current_state == 4:  # Est√°s en S2, seleccionar para volver a M2
            current_state = 1
            display_state(current_state)
        elif current_state == 5:  # Est√°s en S3, seleccionar para volver a M3
            current_state = 2
            display_state(current_state)

        while pin_abajo.value() == 1:
            utime.sleep_ms(10)

    # Verificar el bot√≥n seleccionar
    elif pin_seleccionar.value() == 1:
        if current_state in [0, 1, 2]:  # Est√°s en M1, M2 o M3, puedes ir a S1, S2 o S3 respectivamente
            current_state = current_state + 3
            display_state(current_state)
        elif current_state in [3, 4, 5]:  # Est√°s en S1, S2 o S3, seleccionar para volver a M1, M2 o M3 respectivamente
            current_state = current_state - 3
            display_state(current_state)

        while pin_seleccionar.value() == 1:
            utime.sleep_ms(10)

